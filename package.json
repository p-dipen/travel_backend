{
  "name": "express-rest-api-boilerplate",
  "version": "0.0.0",
  "author": "Lukas Aichbauer <l.aichbauer@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aichbauer/express-rest-api-boilerplate.git"
  },
  "homepage": "https://github.com/aichbauer/express-rest-api-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/aichbauer/express-rest-api-boilerplate/issues"
  },
  "scripts": {
    "start": "npm run nodemon",
    "dev": "cross-env NODE_ENV=development node ./api/api.js",
    "nodemon": "nodemon --exec npm run dev",
    "lint": "eslint ./api/. ./config/. ./test/.",
    "pretest": "npm run lint",
    "production": "cross-env NODE_ENV=production node ./api/api.js",
    "test": "npm run create-sqlite-db; cross-env NODE_ENV=testing jest --coverage; npm run drop-sqlite-db",
    "test-ci": "jest --coverage --forceExit",
    "testing": "NODE_ENV=testing node ./api/api.js"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@types/node": "^13.5.0",
    "aes-js": "^3.1.2",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "chunk": "0.0.2",
    "core-js": "^2.6.9",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "email-templates": "^7.0.4",
    "express": "^4.16.3",
    "express-routes-mapper": "^1.0.2",
    "formidable": "^1.2.2",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.1",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "node-input-validator": "^4.2.0",
    "nodemailer": "^6.4.6",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "qs": "^6.9.1",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.0.0",
    "uuid": "^8.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "nodemon": "^1.17.3",
    "shx": "^0.2.2",
    "supertest": "^3.0.0"
  }
}
